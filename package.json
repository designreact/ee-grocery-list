{
  "name": "ee-grocery-list",
  "version": "0.0.1",
  "description": "Rudimentary grocery list application",
  "main": "bin/server/index.js",
  "scripts": {
    "build": "run-p -c build:*",
    "build:client": "webpack --config webpack.config.js --mode production",
    "build:server": "tsc --build",
    "debug:server": "ts-node ./src/server/index.ts",
    "debug:client": "webpack-dev-server --open --mode development",
    "test": "run-p -c test:*",
    "start": "node ./bin/server/index.js",
    "db:start": "node -e 'require(\"./scripts/dynamodb-local\").start()'",
    "test:unit": "run-p -c test:unit:*",
    "test:unit:react": "jest -c jest-unit-react.config.js",
    "test:unit:js": "jest -c jest-unit.config.js",
    "test:integration": "jest -c jest-integration.config.js",
    "lint": "eslint . --ext [ts,js] --color",
    "format": "lint --fix",
    "precommit": "run-p test lint",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "TBC"
  },
  "engines": {
    "node": "12.x.x"
  },
  "author": "martin@designreact.co.uk",
  "license": "UNLICENCED",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "dependencies": {
    "@koa/cors": "2",
    "aws-sdk": "^2.584.0",
    "axios": "^0.19.0",
    "config": "^3.0.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.19",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1",
    "uuidv4": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@shopify/jest-koa-mocks": "^2.1.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/config": "0.0.34",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "24.0.12",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa__cors": "^3.0.1",
    "@types/node": "10.14.x",
    "@types/supertest": "^2.0.8",
    "@types/uuidv4": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "dynamodb-local": "^0.0.31",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "nock": "10.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.0",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "8.1.0",
